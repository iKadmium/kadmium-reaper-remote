{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "lib": [
      "es6",
      "dom",
      "es2016"
    ],
    "types": [
      "node",
      "core-js"
    ],
    "outDir": "obj",
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}